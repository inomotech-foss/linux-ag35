			gpios = <&pm8019_gpios 1 0>;
			default-state = "off";
			status = "disabled";
		};
	};

	timer@b020000 {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;
		compatible = "arm,armv7-timer-mem";
